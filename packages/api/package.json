{
  "name": "@nl-portal/nl-portal-api",
  "version": "0.1.1-alpha.136",
  "author": "Ritense",
  "license": "EUPL-1.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "pnpm wait && tsc && vite build",
    "clean": "rimraf dist",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "pnpm wait && vite build --watch --mode development",
    "preinstall": "npx only-allow pnpm",
    "wait": "wait-on ../authentication/dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@apollo/client": "3.11.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript-operations": "4.2.3",
    "@graphql-codegen/typescript-react-apollo": "4.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.4",
    "wait-on": "^7.2.0"
  },
  "peerDependencies": {
    "@nl-portal/nl-portal-authentication": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
