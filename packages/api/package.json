{
  "name": "@nl-portal/nl-portal-api",
  "version": "0.2.0-alpha.51",
  "author": "Ritense",
  "license": "EUPL-1.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "pnpm wait && tsc && vite build",
    "clean": "rimraf dist",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "pnpm wait && vite build --watch --mode development",
    "preinstall": "npx only-allow pnpm",
    "wait": "wait-on ../authentication/dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "3.11.10"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript-operations": "4.2.3",
    "@graphql-codegen/typescript-react-apollo": "4.3.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vite": "^5.3.6",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.5",
    "wait-on": "^7.2.0"
  },
  "peerDependencies": {
    "@nl-portal/nl-portal-authentication": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
